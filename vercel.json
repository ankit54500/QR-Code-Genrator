{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/webhook.js": {
      "maxDuration": 30
    },
    "api/cleanup.js": {
      "maxDuration": 60
    }
  }
}
